{
    "name": "bambamboole/laravel-planhat",
    "description": "A laravel package to effortlessly talk to the Planhat API",
    "version": "0.1.0",
    "keywords": [
        "bambamboole",
        "laravel-planhat",
        "api"
    ],
    "homepage": "https://github.com/bambamboole/laravel-planhat",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Manuel Christlieb",
            "email": "manuel@christlieb.eu"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/http": "^9.0|^10.0|^11.0",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.8",
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "Bambamboole\\LaravelPlanhat\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bambamboole\\LaravelPlanhat\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bambamboole\\LaravelPlanhat\\PlanhatServiceProvider"
            ]
        }
    }
}
